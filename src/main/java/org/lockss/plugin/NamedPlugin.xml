<map>
  <entry>
    <string>plugin_au_factory</string>
    <string>org.lockss.plugin.NamedArchivalUnitFactory</string>
  </entry>
  <entry>
    <string>plugin_identifier</string>
    <string>org.lockss.plugin.NamedPlugin</string>
  </entry>
  <entry>
    <string>plugin_name</string>
    <string>Named Plugin</string>
  </entry>
  <entry>
    <string>au_name</string>
    <string>&quot;Named AU: %s&quot;, handle</string>
  </entry>
  <entry>
    <string>plugin_config_props</string>
    <list>
      <org.lockss.daemon.ConfigParamDescr>
        <key>handle</key>
        <displayName>Unique handle</displayName>
        <description>Unique handle for a NamedArchivalUnit</description>
        <type>1</type>
        <size>30</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>features</key>
        <displayName>Features</displayName>
        <description>A semicolon-separated list of features to enable in the NamedArchivalUnit</description>
        <type>1</type>
        <size>30</size>
        <definitional>false</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>start_urls</key>
        <displayName>Start URLs</displayName>
        <description>A semicolon-separated list of URLs from which to begin browsing the AU</description>
        <type>1</type>
        <size>30</size>
        <definitional>false</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>url_stems</key>
        <displayName>URL stems</displayName>
        <description>A semicolon-separated set of stems of URLs with this AU's content</description>
        <type>1</type>
        <size>30</size>
        <definitional>false</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
    </list>
  </entry>

  <!-- Crawl rules include everything, as some non-crawl related parts
       of the daemon (e.g., CU iterators) may exclude CUs that are
       excluded by the crawl rules -->
  <entry>
    <string>au_crawlrules</string>
    <list>
      <string>1,.*</string>
    </list>
  </entry>
  <entry>
    <string>plugin_version</string>
    <string>1</string>
  </entry>
  <entry>
    <string>plugin_feature_version_map</string>
    <map>
      <entry>
        <string>Poll</string>
        <string>1</string>
      </entry>
    </map>
  </entry>
</map>
